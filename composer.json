{
    "name": "andiwijaya/dusk-secure",
    "description": "A more secure Laravel Dusk that provides simple end-to-end testing and browser automation without exposing routes or authentication.",
    "keywords": ["laravel", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andi Wijaya",
            "email": "andyv4@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facebook/webdriver": "~1.0",
        "nesbot/carbon": "~2.34",
        "illuminate/console": "~5.6",
        "illuminate/support": "~5.6",
        "symfony/console": "~4.0",
        "symfony/process": "~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Andiwijaya\\DuskSecure\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andiwijaya\\DuskSecure\\DuskSecureServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
